
@{
   var items = new List<WishList.Models.Item>();
    
}
<h1>Wishlist</h1>
<a asp-action="create">Add item</a>
<ul>
    @{
        foreach (var item in items)
            {
        <li>@item.Description
        <a asp-action="delete" asp-route-id=@item.Id>delete</a>
    </li> 
            }
      
     }
</ul>
